@article{hou_spatio-temporal_2009,
	title = {Spatio-temporal tuning of coherent motion evoked responses in 4–6 month old infants and adults},
	volume = {49},
	issn = {0042-6989},
	url = {http://www.sciencedirect.com/science/article/pii/S0042698909003666},
	doi = {10.1016/j.visres.2009.08.007},
	abstract = {Motion cues provide a rich source of information about translations of the observer through the environment as well as the movements of objects and surfaces. While the direction of motion can be extracted locally these local measurements are, in general, insufficient for determining object and surface motions. To study the development of local and global motion processing mechanisms, we recorded Visual Evoked Potentials (VEPs) in response to dynamic random dot displays that alternated between coherent rotational motion and random motion at 0.8 Hz. We compared the spatio-temporal tuning of the evoked response in 4–6 months old infants to that of adults by recording over a range of dot displacements and temporal update rates. Responses recorded at the frequency of the coherent motion modulation were tuned for displacement at the occipital midline in both adults in infants. Responses at lateral electrodes were tuned for speed in adults, but not in infants. Infant responses were maximal at a larger range of spatial displacement than that of adults. In contrast, responses recorded at the dot-update rate showed a more similar parametric displacement tuning and scalp topography in infants and adults. Taken together, our results suggest that while local motion processing is relatively mature at 4–6 months, global integration mechanisms exhibit significant immaturities at this age.},
	number = {20},
	urldate = {2015-02-25},
	journal = {Vision Research},
	author = {Hou, C. and Gilmore, R. O. and Pettet, M. W. and Norcia, A. M.},
	month = oct,
	year = {2009},
	keywords = {Adolescent, Adult, Aging, Cues, Electroencephalography, Evoked Potentials, Visual, Humans, Infant, Infant vision, Middle Aged, Motion coherence, Motion Perception, Pattern Recognition, Visual, Photic Stimulation, Signal Processing, Computer-Assisted, Visual Cortex, Visual development, Visual evoked potentials, Young Adult},
	pages = {2509--2517},
	file = {Hou et al. - 2009 - Spatio-temporal tuning of coherent motion evoked r.pdf:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/BU3FJ3CC/Hou et al. - 2009 - Spatio-temporal tuning of coherent motion evoked r.pdf:application/pdf;ScienceDirect Full Text PDF:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/MUCZ8GG9/Hou et al. - 2009 - Spatio-temporal tuning of coherent motion evoked r.pdf:application/pdf;ScienceDirect Snapshot:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/MQDIKQM9/Hou et al. - 2009 - Spatio-temporal tuning of coherent motion evoked r.html:text/html}
}

@article{gilmore_development_2007,
	title = {Development of cortical responses to optic flow},
	volume = {24},
	issn = {1469-8714},
	url = {http://journals.cambridge.org/article_S0952523807070769},
	doi = {10.1017/S0952523807070769},
	abstract = {Humans discriminate approaching objects from receding ones shortly after birth, and optic flow associated with self-motion may activate distinctive brain networks, including the human MT+ complex. We sought evidence for evoked brain activity that distinguished radial motion from other optic flow patterns, such as translation or rotation by recording steady-state visual evoked potentials (ssVEPs), in both adults and 4–6 month-old infants to direction-reversing optic flow patterns. In adults, radial flow evoked distinctive brain responses in both the time and frequency domains. Differences between expansion\&sol;contraction and both translation and rotation were especially strong in lateral channels (PO7 and PO8), and there was an asymmetry between responses to expansion and contraction. In contrast, infants\&apos; evoked response waveforms to all flow types were equivalent, and showed no evidence of the expansion\&sol;contraction asymmetry. Infants\&apos; responses were largest and most reliable for the translation patterns in which all dots moved in the same direction. This pattern of response is consistent with an account in which motion processing systems detecting locally uniform motion develop earlier than do systems specializing in complex, globally non-uniform patterns of motion, and with evidence suggesting that motion processing undergoes prolonged postnatal development.},
	number = {06},
	urldate = {2015-02-16},
	journal = {Visual Neuroscience},
	author = {Gilmore, Rick O. and Hou, C. and Pettet, M.W. and Norcia, A.M.},
	month = nov,
	year = {2007},
	keywords = {Development, development, Human, Infants, infants, Optic Flow, Optic flow, V5/MT+},
	pages = {845--856},
	file = {Cambridge Journals Snapshot:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/DFN3I5IC/displayAbstract.html:text/html;Cambridge Journals Snapshot:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/QKPQGG9D/Gilmore et al. - 2007 - Development of cortical responses to optic flow.html:text/html;Gilmore et al. - 2007 - Development of Cortical Responses to Optic Flow.pdf:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/NPZZEII5/Gilmore et al. - 2007 - Development of Cortical Responses to Optic Flow.pdf:application/pdf}
}


@article{jouen_optic_2000,
	title = {Optic flow sensitivity in neonates},
	volume = {23},
	issn = {0163-6383},
	url = {http://www.sciencedirect.com/science/article/pii/S0163638301000443},
	doi = {10.1016/S0163-6383(01)00044-3},
	abstract = {The present research investigates neonatal sensitivity to optic flow. Twenty five 3-day-old infants were placed inside a dark room and observed while presented with a 10 s bilateral and backward peripheral optic motion. Seven constant flow velocity conditions were used (2.5, 5.0, 10.0, 15.0, 20.0, 25.0 and 30.0 degrees per second) and were compared to a baseline motionless condition. Sagittal deviation of the head was computer-sampled at 60 Hz frequency with pressure transducers. As indicated by the mean head pressure during optic flow exposure, infants reacted with backward leaning of the head whose magnitude was linearly related to the optic flow velocity. Additionally, isolated head postural responses were identified. The magnitude of these responses was clearly related to the optic flow velocity.},
	number = {3–4},
	urldate = {2015-02-25},
	journal = {Infant Behavior and Development},
	author = {Jouen, François and Lepecq, Jean-Claude and Gapenne, Olivier and Bertenthal, Bennett I},
	month = mar,
	year = {2000},
	keywords = {Neonate, Optic Flow, Vestibular system},
	pages = {271--284},
	file = {ScienceDirect Full Text PDF:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/UEP3ISH7/Jouen et al. - 2000 - Optic flow sensitivity in neonates.pdf:application/pdf;ScienceDirect Snapshot:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/WXATCSFC/Jouen et al. - 2000 - Optic flow sensitivity in neonates.html:text/html}
}